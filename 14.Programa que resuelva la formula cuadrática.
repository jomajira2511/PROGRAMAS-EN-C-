#include <iostream>
#include <math.h>
using namespace std; 
int main(){
	float a,b,c,positiva,negativa;
	cout<<"ingrese a "<<endl; cin>>a;
	cout<<"ingrese b "<<endl; cin>>b;
	cout<<"ingrese c "<<endl; cin>>c;
	positiva=(((sqrt(pow(b,2)-4*a*c))+-b)/(2*a));
    negativa=(((sqrt(pow(b,2)-4*a*c))+b)/(2*a));
    cout<<"la formula cuadratica positiva es igual a "<<positiva<<endl;
    cout<<"la formula cuadratica negativa es igual a "<<negativa<<endl;
return 0 ;
}
